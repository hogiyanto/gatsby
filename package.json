{
  "name": "gatsby",
  "version": "0.0.1",
  "author": "hogiyanto",
  "description": "Learn gatsby",
  "main": "/src/pages/index.tsx",
  "scripts": {
    "build": "npm run build-prod",
    "build-dev": "GATSBY_ACTIVE_ENV=development gatsby build",
    "build-staging": "GATSBY_ACTIVE_ENV=staging gatsby build",
    "build-prod": "gatsby build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "gatsby develop",
    "start-staging": "GATSBY_ACTIVE_ENV=staging gatsby develop",
    "start-prod": "GATSBY_ACTIVE_ENV=production gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "lint-fix": "eslint --fix src --ext .js,.ts,.tsx",
    "type-check": "tsc",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hogiyanto/gatsby.git"
  },
